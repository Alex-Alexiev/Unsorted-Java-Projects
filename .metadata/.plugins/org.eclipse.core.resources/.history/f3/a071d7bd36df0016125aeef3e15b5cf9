import processing.core.PApplet;
import java.util.Scanner;

public class Main extends PApplet{

	public static void main (String[] args) {
		PApplet.main("Main");
	}
	
	public void settings(){
		size(900, 900);
	}
	
	public void setUp(){
		frameRate(60);
		background(255);
	}
	
	public void draw(){
		background(0);
		strokeWeight(10);
		stroke(0);
		line(100, 100, 100, 100);
	}

}
